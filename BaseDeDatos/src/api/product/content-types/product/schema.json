{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "description": {
      "type": "text"
    },
    "price_provider": {
      "type": "decimal"
    },
    "price_sale": {
      "type": "decimal"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "highlight": {
      "type": "boolean"
    },
    "stock_virtual": {
      "type": "integer"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "permitirSugerirPrecio": {
      "type": "boolean"
    },
    "provider": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::provider.provider",
      "inversedBy": "products"
    },
    "vendido": {
      "type": "boolean",
      "default": false
    },
    "visible": {
      "type": "boolean",
      "default": true
    },
    "subcategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subcategory.subcategory",
      "inversedBy": "products"
    }
  }
}
