{
  "kind": "collectionType",
  "collectionName": "servicios",
  "info": {
    "singularName": "servicio",
    "pluralName": "servicios",
    "displayName": "Servicio"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "descripcion": {
      "type": "text"
    },
    "images": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "provider": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::provider.provider",
      "inversedBy": "servicios"
    },
    "precio_aproximado": {
      "type": "string"
    }
  }
}
